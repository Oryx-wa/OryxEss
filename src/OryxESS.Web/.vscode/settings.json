// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/app/**/*.js": true,
        "**/*.map": true,
        "**/*.csproj": true,
        "**/*.csproj.user": true,
        "**/*.web.Debug.config": true
    },
    // Controls auto save of dirty files. Accepted values:  "off", "afterDelay", "onFocusChange". If set to "afterDelay" you can configure the delay in "files.autoSaveDelay".
    "files.autoSave": "afterDelay",
    "version": "0.1.0",
	"command": "${workspaceRoot}/node_modules/.bin/webpack",
	"isShellCommand": true,
	"args": [
		"--display-modules",
		"--progress"
	]
	
}